<p id="notice"><%= notice %></p>

<h1 style = "font-size: 15px; color:green"><strong>Listing Examples</strong></h1>

<table>
  <thead>
    <tr>
      <th>My file</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody style="background-color: brown; color: yellow">
    <% @examples.each do |example| %>
      <tr>
        <td><a href="<%= example.my_file %>">Dowload</a></td>
        <td><%= link_to 'Show', example %></td>
        <td><%= link_to 'Edit', edit_example_path(example) %></td>
        <td><%= link_to 'Destroy', example, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Example', new_example_path %>
